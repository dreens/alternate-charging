file = {:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/van de Meerakker et al. - 2005 - Higher-order resonances in a Stark decelerator.pdf:pdf},
isbn = {1050-2947$\backslash$n1094-1622},
issn = {1050-2947},

author = {Fabrikant, M. I. and Li, Tian and Fitch, N. J. and Farrow, N. and Weinstein, Jonathan D. and Lewandowski, H. J.},
doi = {10.1103/PhysRevA.90.033418},
eprint = {arXiv:1312.0901v1},
file = {:Users/junye/Library/Application Support/Mendeley Desktop/Downloaded/Fabrikant et al. - 2014 - Method for traveling-wave deceleration of buffer-gas beams of CH.pdf:pdf},
issn = {1050-2947},
journal = {Physical Review A},
month = {sep},
number = {3},
pages = {033418},
title = {{Method for traveling-wave deceleration of buffer-gas beams of CH}},
url = {https://link.aps.org/doi/10.1103/PhysRevA.90.033418},
volume = {90},
year = {2014}
}

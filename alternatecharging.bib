@article{Fabrikant2014,
abstract = {Cryogenic buffer-gas beams are a promising method for producing bright$\backslash$nsources of cold molecular radicals for cold-collision and$\backslash$nchemical-reaction experiments. In order to use these beams in studies of$\backslash$nreactions with controlled collision energies or in trapping experiments,$\backslash$none needs a method of controlling the forward velocity of the beam. A$\backslash$nStark decelerator can be an effective tool for controlling the mean$\backslash$nspeed of molecules produced by supersonic jets, but efficient$\backslash$ndeceleration of buffer-gas beams presents new challenges due to longer$\backslash$npulse lengths. Traveling-wave decelerators are uniquely suited to meet$\backslash$nthese challenges because of their ability to confine molecules in three$\backslash$ndimensions during deceleration and their versatility afforded by the$\backslash$nanalog control of the electrodes. We have created ground-state CH(X-2$\backslash$nPi) radicals in a cryogenic buffer-gas cell with the potential to$\backslash$nproduce a cold molecular beam of 10(11) molecules/pulse. We present a$\backslash$ngeneral protocol for Stark deceleration of beams with a large position$\backslash$nand velocity spread for use with a traveling-wave decelerator. Our$\backslash$nmethod involves confining molecules transversely with a hexapole for an$\backslash$noptimized distance before deceleration. This rotates the phase-space$\backslash$ndistribution of the molecular packet so that the packet is matched to$\backslash$nthe time-varying phase-space acceptance of the decelerator. We$\backslash$ndemonstrate with simulations and an analytic one-dimensional model that$\backslash$nthis method can decelerate a significant fraction of the molecules in$\backslash$nsuccessive wells of a traveling-wave decelerator to produce energy-tuned$\backslash$nbeams for cold and controlled-molecule experiments.},
archivePrefix = {arXiv},
